{
	"name": "my-github-page",
	"version": "0.0.0-semantically-released",
	"description": "My home page",
	"engines": {
		"node": ">=18.18.1"
	},
	"dependencies": {
		"date-fns": "2.30.0",
		"highlight.js": "11.9.0",
		"js-cookie": "3.0.5",
		"marked": "9.1.2",
		"marked-highlight": "2.0.6",
		"parse-md": "3.0.3",
		"zod": "3.22.4"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "1.3.1",
		"@sveltejs/adapter-static": "2.0.3",
		"@sveltejs/kit": "1.25.2",
		"@tsconfig/svelte": "5.0.2",
		"@types/js-cookie": "3.0.4",
		"@types/slug": "5.0.5",
		"@typescript-eslint/eslint-plugin": "6.7.5",
		"@typescript-eslint/parser": "6.7.5",
		"cross-spawn": "7.0.3",
		"dotenv-cli": "7.3.0",
		"eslint": "8.51.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-svelte": "2.34.0",
		"lint-staged": "15.0.1",
		"metascraper": "5.37.1",
		"metascraper-author": "5.37.1",
		"metascraper-date": "5.37.1",
		"metascraper-description": "5.37.1",
		"metascraper-image": "5.37.1",
		"metascraper-logo": "5.37.1",
		"metascraper-publisher": "5.37.1",
		"metascraper-title": "5.37.1",
		"metascraper-url": "5.37.1",
		"postcss": "8.4.31",
		"prettier": "3.0.3",
		"prettier-plugin-svelte": "3.0.3",
		"sass": "1.69.3",
		"semantic-release": "22.0.5",
		"slug": "8.2.3",
		"svelte": "4.2.1",
		"svelte-check": "3.5.2",
		"tslib": "2.6.2",
		"typescript": "5.2.2",
		"vite": "4.4.12"
	},
	"scripts": {
		"meta": "node src/ci/links.js",
		"meta:dev": "dotenv npm run meta",
		"version": "node src/ci/version.js",
		"format": "prettier --write ./**/*.svelte && prettier --write .",
		"format:check": "prettier --plugin-search-dir --check .",
		"lint": "eslint .",
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "dotenv vite dev",
		"preview": "vite preview",
		"start": "node dist",
		"prepare": "svelte-kit sync",
		"draft": "node src/ci/create-md.js"
	},
	"type": "module",
	"keywords": [
		"svelte",
		"typescript",
		"webpack",
		"sass"
	],
	"author": "Sergey Nikitin <srg.post@gmail.com>",
	"license": "MIT",
	"simple-git-hooks": {
		"pre-commit": "npx lint-staged"
	},
	"lint-staged": {
		"*.{md,yml,scss,css,svelte}": [
			"npm run format"
		],
		"*.{js,jsx,ts,tsx}": [
			"npm run format",
			"npm run lint"
		]
	}
}
