{
	"name": "my-github-page",
	"version": "0.0.0-semantically-released",
	"description": "My home page",
	"engines": {
		"node": ">=20.11.0"
	},
	"dependencies": {
		"date-fns": "3.2.0",
		"highlight.js": "11.9.0",
		"js-cookie": "3.0.5",
		"marked": "11.1.1",
		"marked-highlight": "2.1.0",
		"parse-md": "3.0.3",
		"zod": "3.22.4"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "3.0.3",
		"@sveltejs/adapter-static": "3.0.1",
		"@sveltejs/kit": "2.3.2",
		"@sveltejs/vite-plugin-svelte": "3.0.1",
		"@tsconfig/svelte": "5.0.2",
		"@types/js-cookie": "3.0.6",
		"@types/node": "20.11.0",
		"@types/slug": "5.0.7",
		"@typescript-eslint/eslint-plugin": "6.18.1",
		"@typescript-eslint/parser": "6.18.1",
		"cross-spawn": "7.0.3",
		"dotenv-cli": "7.3.0",
		"eslint": "8.56.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.35.1",
		"lint-staged": "15.2.0",
		"metascraper": "5.43.2",
		"metascraper-author": "5.43.1",
		"metascraper-date": "5.43.1",
		"metascraper-description": "5.43.1",
		"metascraper-image": "5.43.1",
		"metascraper-logo": "5.43.1",
		"metascraper-publisher": "5.43.1",
		"metascraper-title": "5.43.1",
		"metascraper-url": "5.43.1",
		"postcss": "8.4.33",
		"prettier": "3.2.1",
		"prettier-plugin-svelte": "3.1.2",
		"sass": "1.69.7",
		"semantic-release": "23.0.0",
		"slug": "8.2.3",
		"svelte": "4.2.8",
		"svelte-check": "3.6.3",
		"tslib": "2.6.2",
		"typescript": "5.3.3",
		"vite": "5.0.11"
	},
	"scripts": {
		"meta": "node src/ci/links.js",
		"meta:dev": "dotenv npm run meta",
		"version": "node src/ci/version.js",
		"format": "prettier --write ./**/*.svelte && prettier --write .",
		"format:check": "prettier --plugin-search-dir --check .",
		"lint": "eslint .",
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "dotenv vite dev",
		"preview": "vite preview",
		"server": "node dist",
		"prepare": "svelte-kit sync",
		"draft": "node src/ci/create-md.js",
		"welcome": "node src/ci/welcome.js",
		"start": "npm run welcome && npm run server"
	},
	"type": "module",
	"keywords": [
		"svelte",
		"typescript",
		"webpack",
		"sass"
	],
	"author": "Sergey Nikitin <srg.post@gmail.com>",
	"license": "MIT",
	"simple-git-hooks": {
		"pre-commit": "npx lint-staged"
	},
	"lint-staged": {
		"*.{md,yml,scss,css,svelte}": [
			"npm run format"
		],
		"*.{js,jsx,ts,tsx}": [
			"npm run format",
			"npm run lint"
		]
	}
}
